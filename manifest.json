{
    "name": "Block",
    "version": "0.0.1",
    "description": "Lightweight website blocker.",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": { 
        "16": "16.png",
        "48": "48.png",
       "128": "128.png" 
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage",
        "tabs",
        "webNavigation"
    ]
}